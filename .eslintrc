{
  "env": {
    "browser": true,
    "jest": true,
    "node": false
  },
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "require-jsdoc" : 0,
    "import/no-named-as-default": 0,
    "jsx-a11y/anchor-is-valid": "off",
    "react/prop-types": [
      0
    ]
  }
}
